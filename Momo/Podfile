# Uncomment the next line to define a global platform for your project
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '8.1'
    
target 'Momo' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  # use_frameworks!

  # Pods for Momo
    # Google api
    # map, places
    pod 'GoogleMaps'
    pod 'GooglePlaces'
    pod 'GooglePlacePicker'
    # analytics
    pod 'Google/Analytics'

    # Facebook api
    pod 'Bolts'
    pod 'FBSDKCoreKit'
    pod 'FBSDKLoginKit'
    pod 'FBSDKShareKit'
    
    # Realm Mobile DB
    pod 'Realm'
    
    pod 'AFNetworking', '~> 3.0'
    pod 'SDWebImage'

    pod 'JSONModel'
    pod 'SVProgressHUD'     # 로딩 팝업
    pod 'FontAwesomeKit/FontAwesome'    # icon fonts

end
